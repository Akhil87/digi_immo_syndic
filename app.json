{
  "expo": {
    "name": "Digi immo Syndic",
    "slug": "digi-immo-syndic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/7e0e5d41-4caa-4c2f-9633-3018e5f19f98"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-localization"
      ]
    ],
    "splash": {
      "image": "./assets/Group.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Group.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devpearl2024.digiimmo",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e0e5d41-4caa-4c2f-9633-3018e5f19f98"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
